@model FoxManager.Models.Student

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Profile page - @Model.StudentName</title>
</head>
<body>
    <h1>@Model.StudentName's profile</h1>
    <ul>
        @foreach (var item in @Model.Projects)
        {
            <li>[@item.ProjectId] - ID @item.DescriptionOfTask - description </li>
        }
    </ul>  

    <h3>@Model.StudentId</h3>
    <h4>@Model.StudentName</h4>

</body>
</html>